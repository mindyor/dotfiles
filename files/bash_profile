source ~/.bashrc

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

alias reload_setup="(z dotfiles && ./setup.sh)"

alias g=git

export NVM_DIR=~/.nvm
  source $(brew --prefix nvm)/nvm.sh

source ~/.z.sh
source ~/.work_related.sh
source ~/.pretty_prompt.sh
source ~/.directory_structure.sh
source ~/.util.sh

source ~/.java_specific.sh

export PATH=/usr/local/sbin:$PATH
